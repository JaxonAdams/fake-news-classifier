{:source-paths ["src/cljs"]

 :dependencies [[reagent/reagent "1.2.0"]]

 :builds {:app {:target :browser
                :output-dir "public/js"
                :asset-path "/js"
                :modules {:main {:init-fn fake-news-classifier.core/init}}
                :compiler-options {:optimizations :advanced}
                :dev {:closure-defines {fake-news-classifier.core/api-gateway-url ""}}
                :release {:closure-defines {fake-news-classifier.core/api-gateway-url ""}}}}}
